# Nango Configuration for Docker Compose
# Copy this file to .env and update the values below

# REQUIRED: 32-character encryption key (generate with: openssl rand -hex 16)
NANGO_ENCRYPTION_KEY=REPLACE_WITH_32_CHAR_ENCRYPTION_KEY

# Database Configuration (defaults work for local development)
NANGO_DB_USER=nango
NANGO_DB_PASSWORD=nango
NANGO_DB_NAME=nango

# Server URLs (defaults work for local development)
NANGO_SERVER_URL=http://localhost:3003
NANGO_PUBLIC_SERVER_URL=http://localhost:3000
SERVER_PORT=3003

# Optional: Dashboard Authentication (leave empty for no auth in development)
NANGO_DASHBOARD_USERNAME=
NANGO_DASHBOARD_PASSWORD=

# Optional: Logging and Features
LOG_LEVEL=info
FLAG_SERVE_CONNECT_UI=true
FLAG_AUTH_ENABLED=false

# MCP OAuth Configuration (for inkeep-management-api)
DEFAULT_OAUTH_CLIENT_ID=mcp-client
OAUTH_REDIRECT_BASE_URL=http://localhost:3002

# OAuth Client Branding (customize OAuth consent screen)
OAUTH_CLIENT_NAME=Inkeep Agent Framework
OAUTH_CLIENT_URI=https://inkeep.com
OAUTH_CLIENT_LOGO_URI=https://storage.googleapis.com/inkeep-static-assets/Favicon_gray_large.png